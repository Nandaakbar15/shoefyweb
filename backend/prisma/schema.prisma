generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url = env("DATABASE_URL")
}

enum Role {
    admin
    pelanggan
}

model User {
    id Int @id @default(autoincrement())
    username String
    email String @unique
    password String
}

model Produk {
    id_produk Int @id @default(autoincrement())
    nama_produk String
    merek_produk String
    stokProduk Int
    hargaProduk Int
    gambar String
}